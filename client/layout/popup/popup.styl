.popupFreeze {
  z-index: 3;
  position: fixed;
  top: 0;
  left: 0;
  
  background-color: white;
  width: 100%;
  height: 100%;
  opacity: 0.7;
}

.popup {
  z-index: 4;
  border: solid #999999 5px;
  padding: 50px;
  background-color: #eeeeee;
  position: fixed;
  top: 5%;
  left: 20%;
  width: 60%;
  max-height: 70%;
  border-radius: 9px;
  
  overflow: auto;
}