.object
  -webkit-touch-callout: none;
  -webkit-user-select: none;
  -khtml-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
  
.objectList, .objectSelect, .objectListDetails, .objectView, .objectEdit, .objectRemove
  border-radius 3px
  padding 8px
  width 400px
  background-color: #eef;
  margin-bottom: 10px;
  width 400px
  
.object .name
  font-weight: 700;

.objectList
  color inherit
  cursor pointer
.objectList .name
  font-weight: 400;
  
.objectListDetails
  background-color: #222
  color: #fff
  cursor pointer

.objectSelect
  background-color: #222
  color: #fff
  cursor pointer
  
.objectView
  color: inherit
  
.objectEdit
  color: inherit
  
.objectRemove
  color: inherit
  
  
.objectAdd {
  background-color: #eef;
  margin-bottom: 20px;
  padding: 8px;
  
  width: 400px;
  font-size: 100%;
}
